@{
    ViewBag.Title = "Data API";
}

@Html.Partial("~/Views/Shared/Jumbotron.cshtml")
@Html.Partial("~/Views/Shared/Code.cshtml")
@Html.Partial("~/Views/Shared/Result.cshtml")

<script>
    document.getElementById('code').innerHTML = 'Encoded:<br>@Html.Raw(ViewBag.JSON)<br><br>Decoded:<br>@HttpUtility.UrlDecode(ViewBag.JSON)';
    document.getElementById('result').innerHTML = library.json.prettyPrint(@Html.Raw(ViewBag.RESULT));
</script>
